<h2>All celebrities</h2>
{{#each allCelebrities}}
<a href="/celebrities/{{this._id}}"><h2>{{this.name}}</h2></a>
<form action="/celebrities/{{this._id}}/delete" method="POST">
<button type="submit">Delete</button>
</form>
<a href="/celebrities/{{this._id}}/edit">Edit</a>
{{/each}}


<a href="/celebrities/new"><h2>Add a new celebrity</h2></a>
